{
  "name": "backend-miosaas",
  "version": "1.0.0",
  "description": "Backend Express.js per app SaaS",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "prod": "npm run build && npm start",
    "migrate": "ts-node src/database/migrations/migrate.ts",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:status": "npm run migrate status",
    "db:health": "npm run migrate health",
    "db:create-tenant": "npm run migrate create-tenant"
  },
  "keywords": [
    "express",
    "typescript",
    "saas",
    "backend"
  ],
  "author": "Tommy Langiano",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.859.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-sts": "^3.864.0",
    "@aws-sdk/credential-providers": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@types/form-data": "^2.2.1",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "aws-jwt-verify": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "slugify": "^1.6.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
